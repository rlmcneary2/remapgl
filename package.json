  "keywords": [
    "map",
    "react",
    "mapbox",
    "mapboxgl",
    "mapbox-gl",
    "reactjs",
    "geography",
    "latitude",
    "longitude",
    "earth"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rlmcneary2/remapgl.git"
  },
  "scripts": {
