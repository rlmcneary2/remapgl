{
  "name": "remapgl",
  "version": "0.9.0",
  "author": "Richard L. McNeary II <rich@limnous.com> (https://limnous.com/)",
  "bugs": {
    "url": "https://github.com/rlmcneary2/remapgl/issues"
  },
  "description": "Quickly and easily create MapboxGL-based maps. Add Markers, Layers, Popups, and other controls to a map using remapgl's React components and props.",
  "homepage": "https://github.com/rlmcneary2/remapgl",
  "keywords": [
    "map",
    "react",
    "mapbox",
    "mapboxgl",
    "mapbox-gl",
    "reactjs",
    "geography",
    "latitude",
    "longitude",
    "earth"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rlmcneary2/remapgl.git"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ]
    ]
  },
  "dependencies": {
    "@mapbox/togeojson": "0.16.0",
    "@turf/bbox": "6.0.1",
    "mapbox-gl": "1.3.0"
  },
  "scripts": {
    "clean": "rimraf ./dist ./.rts2_cache_* ./pkg",
    "doc": "rimraf ./doc && ./node_modules/.bin/typedoc --ignoreCompilerErrors --mode file --name remapgl --out doc",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "bundle": "yarn clean && microbundle --name=remapgl --globals mapbox-gl=mapboxgl",
    "bundle-dev": "yarn clean && microbundle watch  --name=remapgl --globals mapbox-gl=mapboxgl",
    "pika": "pika build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-types": "^0.6.1",
    "@pika/plugin-build-web": "^0.6.1",
    "@pika/plugin-standard-pkg": "^0.6.1",
    "@pika/plugin-ts-standard-pkg": "^0.6.1",
    "@types/jest": "24.0.18",
    "@types/mapbox-gl": "^0.54.3",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "docz": "^2.0.0-rc.63",
    "microbundle": "^0.11.0",
    "np": "^5.0.3",
    "pika-plugin-minify": "^0.1.0",
    "prettier": "1.18.2",
    "react-dom": "^16.11.0",
    "react-scripts": "3.1.1",
    "rimraf": "^3.0.0",
    "tslint": "^5.19.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.6.2"
  },
  "peerDependencies": {
    "react": "16.9.0"
  },
  "files": [
    "dist"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "unpkg": "dist/index.umd.js"
}
