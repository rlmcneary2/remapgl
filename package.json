    "np": "^5.0.3",
    "prettier": "1.18.2",
    "prettier-eslint": "^9.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.2"
  },
  "files": [
    "dist"
  ],
