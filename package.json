{
  "name": "remapgl",
  "version": "0.3.0",
  "author": "Richard L. McNeary II <rich@limnous.com> (https://limnous.com/)",
  "dependencies": {
    "@mapbox/togeojson": "0.16.0",
    "@turf/bbox": "6.0.1",
    "mapbox-gl": "1.3.0"
  },
  "scripts": {
    "clean": "rimraf ./dist ./.rts2_cache_*",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "bundle": "yarn clean && microbundle --name=remapgl --globals mapbox-gl=mapboxgl",
    "bundle-dev": "yarn clean && microbundle watch  --name=remapgl --globals mapbox-gl=mapboxgl"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/mapbox-gl": "^0.54.3",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "microbundle": "^0.11.0",
    "np": "^5.0.3",
    "react-scripts": "3.1.1",
    "rimraf": "^3.0.0",
    "tslint": "^5.19.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.6.2"
  },
  "peerDependencies": {
    "react": "16.9.0"
  },
  "files": [
    "dist"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "np": {
    "contents": "dist"
  }
}
